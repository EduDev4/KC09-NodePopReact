\"\r\n          name=\"email\"\r\n          label=\"phone, email or username\"\r\n          className=\"loginPage-field\"\r\n          value={email}\r\n          onChange={onChange}\r\n        />\r\n        <FormField\r\n          type=\"password\"\r\n          name=\"password\"\r\n          label=\"password\"\r\n          className=\"loginPage-field\"\r\n          value={password}\r\n          onChange={onChange}\r\n        />\r\n        <FormField\r\n          type=\"checkbox\"\r\n          name=\"rememberme\"\r\n          label=\"Remember me!\"\r\n          className=\"loginPage-field\"\r\n          value={rememberme}\r\n          onChange={onChange}\r\n        />\r\n        <br></br>\r\n        <Button\r\n          type=\"submit\"\r\n          className=\"loginPage-submit\"\r\n          variant=\"primary\"\r\n          disabled={!canSubmit()}\r\n        >\r\n          Log in\r\n        </Button>\r\n      </form>\r\n      {error && <div className=\"loginPage-error\">{error.message}</div>}\r\n    </div>\r\n  );\r\n}\r\n\r\nLoginPage.propTypes = {\r\n  onLogin: T.func.isRequired,\r\n  history: T.shape({ push: T.func.isRequired }).isRequired,\r\n};\r\n\r\nexport default LoginPage;\r\n","C:\\Users\\izyed\\Documents\\Keepcoding\\react\\KC09-Reactwitter\\twittereact\\src\\api\\auth.js",[],"C:\\Users\\izyed\\Documents\\Keepcoding\\react\\KC09-Reactwitter\\twittereact\\src\\utils\\storage.js",[],["110","111"],"C:\\Users\\izyed\\Documents\\Keepcoding\\react\\KC09-Reactwitter\\twittereact\\src\\components\\auth\\PrivateRoute.js",[],"C:\\Users\\izyed\\Documents\\Keepcoding\\react\\KC09-Reactwitter\\twittereact\\src\\components\\adverts\\AdvertsPage.js",["112","113"],"C:\\Users\\izyed\\Documents\\Keepcoding\\react\\KC09-Reactwitter\\twittereact\\src\\components\\adverts\\NewAdvertPage.js",["114","115","116","117","118","119"],"C:\\Users\\izyed\\Documents\\Keepcoding\\react\\KC09-Reactwitter\\twittereact\\src\\components\\auth\\Context.js",[],"C:\\Users\\izyed\\Documents\\Kee